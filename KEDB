Problem
=========================
\section scafes_2_1_optional_modules Optional modules like ADOL-C cannot be arbitrarily enabled / disabled in ScaFES 2.0.1

            myCXX='g++' # 4.8.1
            # Distmode:
            myCXXFLAGS="-DNDEBUG -g -Wall -Wextra -Wno-unused -O0";
            #----------------------------------------------------------------------#
            boostBase="/usr"; # 1.53.0-4.1.2
            boostLibdir="/usr/lib64"; # 1.53.0-4.1.2
            #----------------------------------------------------------------------#
            mpiBase="/usr" # 1.7.2-2.1.3
            #----------------------------------------------------------------------#
            netcdfBase="/usr"; # 4.2.1.1-2.1.3
            hdf5Base="/usr"; # 1.8.11-3.2.2
            #----------------------------------------------------------------------#
            # Remark: Built ADOL-C without OpenMP support [KF; 23.11.2012]
            # Otherwise: If OpenMP is not given in "myCXXFLAGS" and configure
            # tests the availability of ADOL-C, then the configure script will fail.
            #adolcBase="/opt/ADOL-C-2.3.0"; # 2.3.0
            adolcBase="/usr"; # 2.4.1-2.1.2

            ../configure \
                  CXX="$myCXX" \
                  CXXFLAGS="$myCXXFLAGS" \
                  --with-adolc=$adolcBase \
                  --with-netcdf=$netcdfBase \
                  --with-mpi=$mpiBase \
                  --enable-mpi=yes \
                  --enable-adolc=no \
                  --enable-openmp=no \
                  --enable-netcdf=no \
                  --with-boost=$boostBase \
                  --with-boost-libdir=$boostLibdir \
                  --enable-boost-mpi=yes \
                  --prefix=/sw/taurus/libraries/scafes/${versionScafes}

